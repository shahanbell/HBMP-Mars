<view class="page" xmlns:wx="http://www.w3.org/1999/xhtml">
  <view class="page__hd">
    <view class="page__title">维修单</view>
  </view>
  <view class="page__bd">
    <form bindsubmit="formSubmit" bindreset="formReset">
      <view class="weui-cells weui-cells_after-title">

        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">填单人员</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="" value="{{employeeId}}-{{employeeName}}" disabled="true" />
          </view>
        </view>

        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">填单部门</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="" value="{{deptName}}" disabled="true" />
          </view>
        </view>



        <view class="weui-cell weui-cell_input weui-cell_vcode">
          <view class="weui-cell__hd">
            <view class="weui-label">叫修单别</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请选择叫修单别" value="{{repairKindName}}" disabled="true" />
          </view>
          <view class="weui-cell__ft">
            <view class="weui-vcode-btn" bindtap="bindRepairKind">…</view>
          </view>
        </view>

        <view class="weui-cell weui-cell_input weui-cell_vcode">
          <view class="weui-cell__hd">
            <view class="weui-label">叫修单号</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请选择叫修单号" value="{{repairno}}" disabled="true" />
          </view>
          <view class="weui-cell__ft">
            <view class="weui-vcode-btn" bindtap="bindRepairnoChange">…</view>
          </view>
        </view>

        <view class="weui-cell weui-cell_input weui-cell_vcode">
          <view class="weui-cell__hd">
            <view class="weui-label">维修人员</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请选择维修人员" value="{{maintaineId}}" disabled="true" />
          </view>
          <view class="weui-cell__ft">
            <view class="weui-vcode-btn" bindtap="bindMaintainNumberSelect">…</view>
          </view>
        </view>

        <view class="weui-cell weui-cell_input weui-cell_vcode">
          <view class="weui-cell__hd">
            <view class="weui-label">维修单别</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请选择维修单别" value="{{maintainTypeName}}" disabled="true" />
          </view>
          <view class="weui-cell__ft">
            <view class="weui-vcode-btn" bindtap="bindMaintainTypeSelect">…</view>
          </view>
        </view>

        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">单据日期</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入单据日期" value="{{ta003}}" disabled="true" />
          </view>
        </view>

        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">客户编号</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入客户编号" value="{{customer}}-{{cusna}}" disabled="true" />
          </view>
        </view>

        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">接单人员</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入接单人员" value="{{ta009}}-{{receiver}}" disabled="true" />
          </view>
        </view>
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">产品品号</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入产品品号" value="{{ta005}}" disabled="true" />
          </view>

        </view>
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">产品品名</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入产品品名" value="{{ta006}}" disabled="true" />
          </view>
        </view>
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">产品规格</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入产品规格" value="{{ta007}}" disabled="true" />
          </view>
        </view>
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">产品序号</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入产品序号" value="{{ta013}}" disabled="true" />
          </view>

        </view>
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">产品机型编号</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入产品机型编号" value="{{ta500}}" disabled="true" />
          </view>
        </view>


        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">产品别</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入产品别" value="{{ta197}}-{{productna}}" disabled="true" />
          </view>
        </view>


        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">区域别</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入区域别" value="{{ta198}}-{{areana}}" disabled="true" />
          </view>
        </view>

        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">问题代号</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入问题代号" value="{{ta071}}" disabled="true" />
          </view>
        </view>

        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell">
            <view class="weui-label">问题描述</view>
            <view class="weui-cell__bd">
              <textarea class="weui-textarea" placeholder="请输入问题描述" value="{{ta010}}" style="height: 100rpx" disabled="true" />
            </view>
          </view>

          <view class="weui-cell">
            <view class="weui-label">备注</view>
            <view class="weui-cell__bd">
              <textarea class="weui-textarea" placeholder="请输入问题描述" value="{{remark}}" style="height: 100rpx" disabled="true" />
            </view>
          </view>
        </view>

        <view class="weui-cells__title">录入库存交易单</view>



        <view class="weui-cell weui-cell_input weui-cell_vcode">
          <view class="weui-cell__hd">
            <view class="weui-label">单别</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请选择录入库存交易单别" value="{{incentoryformName}}" disabled="true" />
          </view>
          <view class="weui-cell__ft">
            <view class="weui-vcode-btn" bindtap="bindIncentoryformSelect">…</view>
          </view>
        </view>


        <view class="weui-cell weui-cell_input weui-cell_vcode">
          <view class="weui-cell__hd">
            <view class="weui-label">交易原因</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请选择交易原因" value="{{tradingreasonName}}" disabled="true" />
          </view>
          <view class="weui-cell__ft">
            <view class="weui-vcode-btn" bindtap="bindTradingReasonSelect">…</view>
          </view>
        </view>


        <view class="weui-cell weui-cell_input weui-cell_vcode">
          <view class="weui-cell__hd">
            <view class="weui-label">发货部门</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请选择叫修单别" value="{{deliverydeptName}}" disabled="true" />
          </view>
          <view class="weui-cell__ft">
            <view class="weui-vcode-btn" bindtap="bindDeliverydeptSelect">…</view>
          </view>
        </view>



  

          <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell">
        <view class="weui-label">备注</view>
          <view class="weui-cell__bd">
            <textarea class="weui-textarea" placeholder="请输入录入库存交易单备注"style="height: 100rpx" bindblur="bindDeliveryremarkChange" />
          </view>
        </view>
      </view>



        <view class="weui-cells__title">维修明细</view>
        <view class="weui-panel weui-panel_access" wx:for="{{detailList}}" wx:key="seq">

          <view class="weui-panel__hd">
            <view>维修明细{{item.seq}}
              <view class="button-warn" bindtap="bindRemoveDetailTap" data-index="{{index}}">删除</view>
              <view class="button-default" bindtap="bindEditDetailTap" data-index="{{index}}">修改</view>
            </view>
          </view>

          <view class="weui-panel__bd">


            <view class="weui-form-preview__bd">
              <view class="weui-form-preview__item">
                <view class="weui-form-preview__label">产品品号</view>
                <view class="weui-form-preview__value">{{item.productQuality}}</view>
              </view>
            </view>

            <view class="weui-form-preview__bd">
              <view class="weui-form-preview__item">
                <view class="weui-form-preview__label">产品品名</view>
                <view class="weui-form-preview__value">{{item.product_name}}</view>
              </view>
            </view>

            <view class="weui-form-preview__bd">
              <view class="weui-form-preview__item">
                <view class="weui-form-preview__label">产品规格</view>
                <view class="weui-form-preview__value">{{item.productStandard}}</view>
              </view>
            </view>

            <view class="weui-form-preview__bd">
              <view class="weui-form-preview__item">
                <view class="weui-form-preview__label">仓库</view>
                <view class="weui-form-preview__value">{{item.warehouseName}}</view>
              </view>
            </view>

            <view class="weui-form-preview__bd">
              <view class="weui-form-preview__item">
                <view class="weui-form-preview__label">预计维修数量</view>
                <view class="weui-form-preview__value">{{item.planMaintainCount}}</view>
              </view>
            </view>

          </view>
        </view>
      </view>
      <view class="button-sp-area">
        <button class="weui-btn" bindtap="bindAddDetailTap">添加明细</button>
      </view>
      <view class="button-sp-area">
        <button class="weui-btn" form-type="submit" type="primary" disabled="{{!canSubmit}}">提交</button>
      </view>
    </form>
  </view>
</view>