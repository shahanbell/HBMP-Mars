/* component/eqpDocList/eqpDocList.wxss */

.swiper-tab{  
  width: 100%;  
  border-top: 2rpx solid #eee;  
  border-bottom: 2rpx solid #eee;  
  text-align: center;  
  line-height: 88rpx;
  background: #fff;  
  /*position: fixed;*/
  /*top: 0;  */
  z-index: 999;
}  
.swiper-tab-list{  
font-size:32rpx;  
display: inline-block;  
width: 25%;  
color: #333; 
/* font-weight: bold;  */
}
.navTabItem{
position: relative; 
color: #E22E1F;
}  
.navTabItem::after{
position: absolute; 
left: 50%;
bottom: 0;
transform: translateX(-50%);
-webkit-transform: translateX(-50%);
width:120rpx;
height:6rpx;
content: '';
background:linear-gradient(262deg,rgba(255,94,81,0.88),rgba(226,46,31,0.88));
border-radius:3rpx;
}  


.swiper-box{ 
display: block; 
height: 100%; 
width: 100%; 
/*margin-top: 90rpx;*/
overflow: hidden; 
background-color: #F8F8F8;
}  
page{
background-color: #F2F2F2;
}
.proList{
width: 710rpx;
height: 313rpx;
border-radius:16rpx;
background-color: #fff; 
margin-bottom: 20rpx;
margin-left:auto;
margin-right:auto;
padding: 30rpx 24rpx 0 24rpx;
box-sizing: border-box;
}
.proTitle,.proInfo,.proFoot{
display: flex;
justify-content: space-between;
}
.proTitle_id{
font-size: 26rpx;
color: #333;
}
.proTitle_id image{
width: 32rpx;
height: 32rpx;
margin-right: 15rpx;
position: relative;
top: 8rpx;
/* margin-top: 15rpx; */
}
.proTitle .state{
font-size: 26rpx;
}
.proTitle .state_normal{
color: #FF4B0A;
}
.proTitle .state_highLight{
color: #D32835;
}
.proInfo{
padding: 30rpx 0;
box-sizing: border-box;
}
.proInfo_img{
width:110rpx;
height:110rpx;
border-radius:10rpx;
overflow: hidden;
/* background-color: pink; */
}
.proInfo_right{
width: 522rpx;
padding-top: 12rpx;
box-sizing: border-box;
}
.proInfo_text{
display: flex;
justify-content: space-between;
justify-items: middle;
}
.proInfo_text_title{
font-size: 28rpx;
color: #000;
line-height: 45rpx;
white-space: nowrap;
max-width: 480rpx;
text-overflow: ellipsis;
overflow: hidden;
margin-bottom: 10rpx;
}
.proInfo_text_title .iconfont{
font-size: 30rpx;
}
.icon-qianjinjiantou{
font-size: 25rpx;
color: #000;
font-weight: bold;
}
.proInfo_text_dtt{
font-size: 24rpx;
color: #999; 
}
.proInfo_text_acount{
font-size: 26rpx;
color: #999; 
}
.proInfo_text_acount text{
font-size: 32rpx;
color: #000; 
}
.proFoot{
font-size: 24rpx;
color: #333; 
line-height: 82rpx;
border-top: 2rpx solid #eee;
white-space: nowrap;
}
.proFoot text{
position: relative;
font-size: 26rpx;
color: #000; 
}

.searchRightIcon{
display: flex;
justify-content: center;
}

.filterInput{
font-size: 27rpx;
border-radius: 18px;
width: 200rpx;
border: 1rpx solid rgb(240, 240, 240);
background-color: rgb(240, 240, 240);
height: 70rpx;
display: flex;
justify-content: space-between;
align-items: center;
text-align: center;
}

.timeFilterBlock{
display: flex;
justify-content: center;  
align-items: center;
border-top:0px;
}

.buttonBlock{
display: flex;
justify-content: center;  
align-items: center;
border-top:0px;
margin-top: 70px;
}

.line{
/*margin: 0 auto;*/
margin-left: 8rpx;
margin-right: 8rpx;
width: 28rpx;
height: 2rpx;
/*height: 2rpx;*/
background-color: rgb(41, 29, 29);
}

.startView{
  margin-bottom: 0px;
  height: 0px;

  /* width: 28rpx;
  height: 2rpx;
  background-color: rgb(41, 29, 29); */
}

.endView{
  margin-bottom: 0px;
  height: 0px;

  /* width: 28rpx;
  height: 2rpx;
  background-color: rgb(41, 29, 29); */
}

.scrollTest{
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
  width: 100%;
  box-sizing: border-box;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  height: 100%;
  overflow: visible;
  
}

.proListWrap{
  margin-top: 1rpx;
  width: 100%;
  height: 20rpx;
}
